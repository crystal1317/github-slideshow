# EditorConfig is awesome: http://EditorConfig.org
root = false

[*]
indent_style = tab
indent_size = 4
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = false
charset = utf-8

[*.{json,js,css,scss,yml,htm,html}]
indent_style = space
indent_size = 2

[*.{md,mdown,markdown}]
trim_trailing_whitespace = false
insert_final_newline = true
indent_style = space
indent_size = 4



